generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PG_DATABASE_URL")
}

model TaxReport {
  id Int @default(autoincrement()) @id
  fiscalQuarter Int
  fiscalYear Int
}

model File {
  id Int @default(autoincrement()) @id
  fileName String
  fileDestination String
}
