{
  "name": "tax-report-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:up:dev": "dotenv -e .env.dev -- docker-compose -f docker-compose-dev.yml up -d",
    "docker:up:test": "dotenv -e .env.test -- docker-compose -f docker-compose-test.yml up -d",
    "docker:down:dev": "dotenv -e .env.dev -- docker-compose -f docker-compose-dev.yml down",
    "docker:down:test": "dotenv -e .env.test -- docker-compose -f docker-compose-test.yml down",
    "migration:create:dev": "dotenv -e .env.dev -- prisma migrate dev",
    "migration:create:test": "dotenv -e .env.test -- prisma migrate dev",
    "migration:deploy:dev": "dotenv -e .env.dev -- prisma migrate deploy",
    "migration:deploy:test": "dotenv -e .env.test -- prisma migrate deploy",
    "migration:deploy:prod": "dotenv -e .env.prod -- prisma migrate deploy",
    "studio:dev": "dotenv -e .env.dev -- prisma studio",
    "studio:test": "dotenv -e .env.test -- prisma studio",
    "studio:prod": "dotenv -e .env.prod -- prisma studio",
    "start:dev": "dotenv -e .env.dev -- nest start --watch",
    "start:test": "dotenv -e .env.test -- nest start --watch",
    "start:prod": "dotenv -e .env.prod -- nest start --watch",
    "build:dev": "dotenv -e .env.dev -- nest build",
    "build:test": "dotenv -e .env.test -- nest build",
    "build:prod": "dotenv -e .env.prod -- nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test -- jest --config jest.json --runInBand",
    "test:watch": "dotenv -e .env.test -- jest --config jest.json --runInBand --watch",
    "test:unit": "dotenv -e .env.test -- jest",
    "test:int": "dotenv -e .env.test -- jest --config jest-int.json --runInBand",
    "test:e2e": "dotenv -e .env.test -- jest --config jest-e2e.json --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@prisma/client": "^4.8.1",
    "currency.js": "^2.0.4",
    "date-fns": "^2.30.0",
    "fs-extra": "^11.1.0",
    "reflect-metadata": "^0.1.13",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.0.1",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "prisma": "^4.8.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  }
}
